{
  "crons": [
    { "path": "/api/cron_gsr", "schedule": "5 0 * * *" }
  ],
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
